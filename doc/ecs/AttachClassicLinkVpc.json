{
  "name": "AttachClassicLinkVpc",
  "protocol": "HTTP|HTTPS",
  "method": "POST|GET",
  "path": "/",
  "summary_cn": "调用AttachClassicLinkVpc将一台经典网络类型实例连接到专有网络VPC中，使经典网络类型实例可以和VPC中的云资源私网互通。",
  "abc_id": 59021,
  "timeout": 10000,
  "reqFormat": {
    "RegionId": {
      "name": "RegionId",
      "type": "String",
      "position": "Query",
      "checkBlank": "false",
      "required": true,
      "docRequired": false,
      "example_cn": "cn-hangzhou",
      "description_cn": "实例所属的地域ID。您可以调用[DescribeRegions](~~25609~~)查看最新的阿里云地域列表。 "
    },
    "InstanceId": {
      "name": "InstanceId",
      "type": "String",
      "position": "Query",
      "checkBlank": "false",
      "required": true,
      "docRequired": false,
      "example_cn": "i-test",
      "description_cn": "经典网络类型实例ID。您可以调用[DescribeInstances](~~25506~~)查看您可用的实例。 "
    },
    "VpcId": {
      "name": "VpcId",
      "type": "String",
      "position": "Query",
      "checkBlank": "false",
      "required": true,
      "docRequired": false,
      "example_cn": "vpc-test",
      "description_cn": "开启ClassicLink的VPC ID。您可以调用[DescribeVpcs](~~35739~~)查看您可用的VPC。 "
    },
    "SourceRegionId": {
      "name": "SourceRegionId",
      "type": "String",
      "position": "Host",
      "checkBlank": "false",
      "required": false,
      "docRequired": false,
      "example_cn": "hide"
    },
    "OwnerId": {
      "name": "OwnerId",
      "type": "Long",
      "position": "Query",
      "checkBlank": "false",
      "required": false,
      "docRequired": false,
      "example_cn": "155780923770",
      "description_cn": "RAM 用户的虚拟账号 ID。"
    }
  },
