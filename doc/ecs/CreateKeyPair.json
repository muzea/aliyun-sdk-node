{
  "name": "CreateKeyPair",
  "protocol": "HTTP|HTTPS",
  "method": "POST|GET",
  "path": "/",
  "summary_cn": "调用CreateKeyPair创建一对SSH密钥对。我们会为您保管密钥的公钥部分，并返回未加密的PEM编码的PKCS#8格式私钥。您需要自行妥善保管私钥部分。",
  "abc_id": 51771,
  "timeout": 5000,
  "reqFormat": {
    "RegionId": {
      "name": "RegionId",
      "type": "String",
      "position": "Query",
      "checkBlank": "false",
      "required": true,
      "docRequired": false,
      "example_cn": "cn-hangzhou",
      "description_cn": "密钥对所在的地域ID。您可以调用[DescribeRegions](~~25609~~)查看最新的阿里云地域列表。"
    },
    "KeyPairName": {
      "name": "KeyPairName",
      "type": "String",
      "position": "Query",
      "checkBlank": "false",
      "required": true,
      "docRequired": false,
      "example_cn": "FinanceJoshuaV23",
      "description_cn": "密钥对名称。长度为2~128个英文或中文字符。必须以大小字母或中文开头，不能以http://和https://开头。可以包含数字、半角冒号（:）、下划线（_）或者连字符（-）。"
    },
    "SourceRegionId": {
      "name": "SourceRegionId",
      "type": "String",
      "position": "Host",
      "checkBlank": "false",
      "required": false,
      "docRequired": false,
      "example_cn": "hide"
    },
    "OwnerId": {
      "name": "OwnerId",
      "type": "Long",
      "position": "Query",
      "checkBlank": "false",
      "required": false,
      "docRequired": false,
      "example_cn": "155780923770",
      "description_cn": "RAM用户的虚拟账号ID。"
    },
    "Tag": {
      "name": "Tag",
      "type": "RepeatList",
      "position": "Query",
      "required": false,
      "docRequired": false,
      "subType": "Json",
      "params": [
        {
          "name": "Key",
          "type": "String",
          "required": false,
          "docRequired": false
        },
        {
          "name": "Value",
          "type": "String",
          "required": false,
          "docRequired": false
        }
      ]
    },
    "ResourceGroupId": {
      "name": "ResourceGroupId",
      "type": "String",
      "position": "Query",
      "checkBlank": "false",
      "required": false,
      "docRequired": false,
      "example_cn": "rg-resourcegroupid1",
      "description_cn": "SSH密钥对所在的企业资源组ID。"
    }
  },
  "res": {
    "RequestId": "473469C7-AA6F-4DC5-B3DB-A3DC0DE3C83E",
    "KeyPairName": "test",
    "KeyPairFingerPrint": "89:f0:ba:62:ac:b8:aa:e1:61:5e:fd:81:69:86:6d:6b:f0:c0:5a:d7",
    "PrivateKeyBody": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
  }
}