{
  "name": "CreateMonitorGroupNotifyPolicy",
  "protocol": "HTTPS|HTTP",
  "method": "POST|GET",
  "path": "/",
  "summary_cn": "创建暂停应用分组报警通知的策略。",
  "abc_id": 115014,
  "timeout": 10000,
  "reqFormat": {
    "RegionId": {
      "name": "RegionId",
      "type": "String",
      "position": "System",
      "checkBlank": "false",
      "required": true,
      "docRequired": false,
      "example_cn": "cn-hangzhou",
      "description_cn": "资源所属的地域ID。"
    },
    "PolicyType": {
      "name": "PolicyType",
      "type": "String",
      "position": "Query",
      "checkBlank": "true",
      "visibility": "Public",
      "required": true,
      "docRequired": false,
      "example_cn": "PauseNotify",
      "description_cn": "暂停通知类型， 目前仅支持： PauseNotify。"
    },
    "GroupId": {
      "name": "GroupId",
      "type": "String",
      "position": "Query",
      "checkBlank": "false",
      "visibility": "Public",
      "required": true,
      "docRequired": false,
      "example_cn": "12345",
      "description_cn": "应用分组ID。"
    },
    "StartTime": {
      "name": "StartTime",
      "type": "Long",
      "position": "Query",
      "checkBlank": "false",
      "visibility": "Public",
      "required": true,
      "docRequired": false,
      "example_cn": "1551756547863",
      "description_cn": "暂停通知的开始时间，距离1970年1月1日 00:00:00的毫秒数。"
    },
    "EndTime": {
      "name": "EndTime",
      "type": "Long",
      "position": "Query",
      "checkBlank": "false",
      "visibility": "Public",
      "required": true,
      "docRequired": false,
      "example_cn": "1551757147863",
      "description_cn": "暂停通知的结束时间，距离1970年1月1日 00:00:00的毫秒数。"
    }
  },
  "res": {
    "Result": 1,
    "Success": true,
    "Code": "200"
  }
}