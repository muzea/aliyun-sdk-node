{
  "name": "CreateUserCertificate",
  "protocol": "HTTP|HTTPS",
  "method": "POST|GET",
  "path": "/",
  "summary_cn": "添加证书。",
  "abc_id": 126557,
  "timeout": 10000,
  "reqFormat": {
    "RegionId": {
      "name": "RegionId",
      "type": "String",
      "position": "Query",
