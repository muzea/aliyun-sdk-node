{
  "name": "UpdateJob",
  "protocol": "HTTPS",
  "method": "PUT",
  "path": "/api/v2/projects/[projectName]/jobs/[jobName]",
  "summary_cn": "更新作业相关属性",
  "abc_id": 0,
  "timeout": 10000,
  "reqFormat": {
    "RegionId": {
      "name": "RegionId",
      "type": "String",
      "position": "Header",
      "checkBlank": "false",
      "visibility": "Public",
      "required": false,
      "docRequired": false,
      "example_cn": "cn-hangzhou-pre",
      "description_cn": "集团内用户使用： cn-hangzhou-pre 公共云预发； cn-hangzhou-internal 集团内生产； cn-hangzhou-internal-pre 集团内预发；"
    },
    "jobName": {
      "name": "jobName",
      "type": "String",
      "position": "Path",
      "checkBlank": "false",
      "visibility": "Public",
      "required": true,
      "docRequired": true,
      "example_cn": "job1",
      "description_cn": "作业名称"
    },
    "planJson": {
      "name": "planJson",
      "type": "String",
      "position": "Body",
      "checkBlank": "false",
      "visibility": "Public",
      "required": false,
      "docRequired": true,
      "example_cn": "{a:b}",
      "description_cn": "作业执行计划"
    },
    "code": {
      "name": "code",
      "type": "String",
      "position": "Body",
      "checkBlank": "false",
      "visibility": "Public",
      "required": false,
      "docRequired": true,
      "example_cn": "code",
      "description_cn": "作业代码"
    },
    "engineVersion": {
      "name": "engineVersion",
      "type": "String",
      "position": "Body",
      "checkBlank": "false",
      "visibility": "Public",
      "required": false,
      "docRequired": true,
      "example_cn": "blink_2.2.4",
      "description_cn": "引擎版本"
    },
    "clusterId": {
      "name": "clusterId",
      "type": "String",
      "position": "Body",
      "checkBlank": "false",
      "visibility": "Public",
      "required": false,
      "docRequired": true,
      "example_cn": "d6wxwo5tnrmuamx2ly3m7vkz",
      "description_cn": "集群ID"
    },
    "queueName": {
      "name": "queueName",
      "type": "String",
      "position": "Body",
      "checkBlank": "false",
      "visibility": "Public",
      "required": false,
      "docRequired": true,
      "example_cn": "root.default",
      "description_cn": "队列名称"
    },
    "packages": {
      "name": "packages",
      "type": "String",
      "position": "Body",
      "checkBlank": "false",
      "visibility": "Public",
      "required": false,
      "docRequired": false,
      "example_cn": "package1.jar",
      "description_cn": "package名称"
    },
    "projectName": {
      "name": "projectName",
      "type": "String",
      "position": "Path",
      "checkBlank": "false",
      "visibility": "Public",
      "required": true,
      "docRequired": true,
      "example_cn": "project1",
      "description_cn": "项目名称"
    },
    "description": {
      "name": "description",
      "type": "String",
      "position": "Body",
      "checkBlank": "false",
      "visibility": "Public",
      "required": false,
      "docRequired": false,
      "example_cn": "test",
      "description_cn": "作业备注描述"
    },
    "folderId": {
      "name": "folderId",
      "type": "Long",
      "position": "Body",
      "checkBlank": "false",
      "visibility": "Public",
      "required": false,
      "docRequired": false,
      "example_cn": "123",
      "description_cn": "文件夹ID"
    },
    "properties": {
      "name": "properties",
      "type": "String",
      "position": "Body",
      "checkBlank": "false",
      "visibility": "Public",
      "required": false,
      "docRequired": false,
      "example_cn": "k:v",
      "description_cn": "作业运行配置参数"
    }
  },
  "res": {}
}